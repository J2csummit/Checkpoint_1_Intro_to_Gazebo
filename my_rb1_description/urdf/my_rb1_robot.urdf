<?xml version="1.0" ?>

<robot name="RB1"> 

    <!-- Gazebo Tags -->

    <!-- Gazebo Frictions-->
    <gazebo reference="front_caster">
        <material>Gazebo/Grey</material>
        <mu1>0.0</mu1>
        <mu2>0.0</mu2>
    </gazebo>
    <gazebo reference="back_caster">
        <material>Gazebo/Grey</material>
        <mu1>0.0</mu1>
        <mu2>0.0</mu2>
    </gazebo>

    <!-- Base Footprint -->
    <link name="base_footprint">
    </link>

    <!-- Base Link -->
    <joint name="joint_footprint_link" type="fixed">
        <origin xyz="0.0 0.0 0.15" rpy="0.0 0.0 0.0"/>
        <parent link="base_footprint"/>
        <child link="base_link"/>
        <joint_properties damping="1.0" friction="1.0" />
    </joint>
    <link name="base_link">
        <inertial>
            <mass value="21.0"/>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <inertia ixx="0.375375" ixy="0.0" ixz="0.0" iyy="0.375375" iyz="0.0" izz="0.65625"/>
        </inertial>
        <collision>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <geometry>
                <cylinder radius="0.25" length="0.3"/>
            </geometry>
        </collision>
        <visual>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <geometry>
                <cylinder radius="0.25" length="0.3"/>
            </geometry>
            <material name="DarkBlue">
                <color rgba="0.2 0.2 0.4 1" />
            </material>
        </visual>
    </link>

    <!-- Right Wheel -->
    <joint name="joint_link_right_wheel" type="continuous">
        <origin rpy="0 0 0" xyz="0.0 -0.2 -0.15" />
        <parent link="base_link"/>
        <child link="right_wheel"/>
        <axis xyz="0.0 1.0 0.0" rpy="0.0 0.0 0.0"/>
        <limit effort="10000.0" velocity="1000.0"/>
        <joint_properties damping="1.0" friction="1.0" />
    </joint>
    <link name="right_wheel">
        <inertial>
            <mass value="1.0"/>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <inertia ixx="0.000157552" ixy="0.0" ixz="0.0" iyy="0.000157552" iyz="0.0" izz="0.0003125"/>
        </inertial>
        <collision>
            <origin xyz="0.0 0.0 0.0" rpy="1.5707 0.0 0.0"/>
            <geometry>
                <cylinder radius="0.025" length="0.025"/>
            </geometry>
        </collision>
        <visual>
            <origin xyz="0.0 0.0 0.0" rpy="1.5707 0.0 0.0"/>
            <geometry>
                <cylinder radius="0.025" length="0.025"/>
            </geometry>
            <material name="Red">
                <color rgba="1 0 0 1" />
            </material>
        </visual>
    </link>

    <!-- Left Wheel -->
    <joint name="joint_link_left_wheel" type="continuous">
        <origin rpy="0 0 0" xyz="0.0 0.2 -0.15" />
        <parent link="base_link"/>
        <child link="left_wheel"/>
        <axis xyz="0.0 1.0 0.0" rpy="0.0 0.0 0.0"/>
        <limit effort="10000.0" velocity="1000.0"/>
        <joint_properties damping="1.0" friction="1.0" />
    </joint>
    <link name="left_wheel">
        <inertial>
            <mass value="1.0"/>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <inertia ixx="0.000157552" ixy="0.0" ixz="0.0" iyy="0.000157552" iyz="0.0" izz="0.0003125"/>
        </inertial>
        <collision>
            <origin xyz="0.0 0.0 0.0" rpy="1.5707 0.0 0.0"/>
            <geometry>
                <cylinder radius="0.025" length="0.025"/>
            </geometry>
        </collision>
        <visual>
            <origin xyz="0.0 0.0 0.0" rpy="1.5707 0.0 0.0"/>
            <geometry>
                <cylinder radius="0.025" length="0.025"/>
            </geometry>
            <material name="Red">
                <color rgba="1 0 0 1" />
            </material>
        </visual>
    </link>

    <!-- Front Caster -->
    <joint name="joint_link_front_caster" type="fixed">
        <origin xyz="0.1 0.0 -0.15" rpy="0.0 0.0 0.0"/>
        <parent link="base_link"/>
        <child link="front_caster"/>
        <joint_properties damping="0.0" friction="0.0" />
    </joint>
    <link name="front_caster">
        <inertial>
            <mass value="1" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="0.00025" ixy="0" ixz="0" iyy="0.00025" iyz="0" izz="0.00025"/>
        </inertial>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <sphere radius="0.025" />
            </geometry>
        </collision>
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <sphere radius="0.025" />
            </geometry>
            <material name="Grey">
                <color rgba="0.6 0.6 0.6 1" />
            </material>
        </visual>
    </link>

    <!-- Back Caster -->
    <joint name="joint_link_back_caster" type="fixed">
        <origin xyz="-0.1 0.0 -0.15" rpy="0.0 0.0 0.0"/>
        <parent link="base_link"/>
        <child link="back_caster"/>
        <joint_properties damping="0.0" friction="0.0" />
    </joint>
    <link name="back_caster">
        <inertial>
            <mass value="1" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="0.00025" ixy="0" ixz="0" iyy="0.00025" iyz="0" izz="0.00025"/>
        </inertial>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <sphere radius="0.025" />
            </geometry>
        </collision>
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <sphere radius="0.025" />
            </geometry>
            <material name="Grey">
                <color rgba="0.6 0.6 0.6 1" />
            </material>
        </visual>
    </link>

    <!-- Front Lazer -->
    <joint name="joint_link_front_laser" type="fixed">
        <origin xyz="0.25 0 0.075" rpy="3.14159 0.0 0.0"/>
        <parent link="base_link"/>
        <child link="front_laser"/>
        <joint_properties damping="1.0" friction="1.0" />
    </joint>
    <link name="front_laser">
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <mass value="0.001" />
            <inertia ixx="0.0" ixy="0" ixz="0" iyy="0.0" iyz="0" izz="0.0"/>
        </inertial>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <cylinder radius="0.01" length="0.01"/>
            </geometry>
        </collision>
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <cylinder radius="0.01" length="0.01"/>
            </geometry>
            <material name="Red">
                <color rgba="0.7 0.1 0.1 1" />
            </material>
        </visual>
    </link>

</robot>